import java.util.*;
class TwoNumbersSum
{
    public static void main(String[] args)
    {
        Scanner s = new Scanner(System.in);
        System.out.print("Enter two numbers a and b to be added : ");
        int a = s.nextInt();
        int b = s.nextInt();
        int sum = a + b;
        //System.out.println(sum);
        System.out.println("Sum of " + a + " and " + b + " is " + sum);
    }
}
