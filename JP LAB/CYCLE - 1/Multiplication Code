import java.util.*;
class Multiplication
{
    public static void main(String[] args)
    {
        Scanner s = new Scanner(System.in);
        System.out.print("Enetr values of a and b to be multiplied : ");
        int a = s.nextInt();
        int b = s.nextInt();
        System.out.println(a+" * "+b+" = "+(a*b));
    }
}
